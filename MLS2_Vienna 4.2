conf t
hostname MLS2_Vienna
username cisco privilege 15 secret class
banner login !Efetue um backup de segurança antes de efetuar alteracoes!
ip domain name MLS2_Vienna.CCNA_lab.com
crypto key generate rsa general-keys modulus 1024 expo
line con 0
login local
exit
line vty 0 4
privilege level 15
login local
transport input ssh
exit

! ----- Services
ip dhcp snooping

service password-encryption

!---- VLANS
vlan 10

name External_Servers

vlan 11
name Internal_Servers

vlan 200

name Voice

vlan 100

name Engineering

vlan 101

name Administration

vlan 254

name Native

vlan 255

name Management

vlan 299

name BlackHole





interface vlan 10
description External_Servers
ip add 172.19.8.253 255.255.254.0
standby 10 ip 172.19.9.254                  !- o ip a ser usado que é o ultimo ip da rede (/23)
Standby 10 priority 100
Standby 10 preempt
Exit

Interface vlan 11
description Internal_Servers
ip add 172.19.10.253 255.255.254.0
standby 101 ip 172.19.11.254
Standby 101 priority 100
Standby 101 preempt
Exit

Interface vlan 255
description BlackHole
ip add 172.19.12.253 255.255.255.0
standby 255 ip 172.19.12.254
Standby 255 priority 100
Standby 255 preempt
Exit

int vlan 100
description Engineering
ip add 172.19.4.253 255.255.254.0
standby 100 ip 172.19.5.254
Standby 100 priority 105
ip helper-address 172.19.10.1
exit

int vlan 101
description Administration
ip add 172.19.6.253 255.255.254.0
standby 101 ip 172.19.7.254
Standby 101 priority 105
Standby 101 preempt
ip helper-address 172.19.10.1
exit

int vlan 200
description Voice
ip add 172.19.0.253 255.255.252.0
standby 200 ip 172.19.3.254               !- este é /22 logo o ultimo ip e .3.254
Standby 200 priority 105
Standby 200 preempt
ip helper-address 172.19.10.1
exit
