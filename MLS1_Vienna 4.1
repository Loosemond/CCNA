! ----- Services
ip dhcp snooping

service password-encryption

!---- VLANS
vlan 10

name External_Servers

vlan 11
name Internal_Servers

vlan 200

name Voice

vlan 100

name Engineering

vlan 101

name Administration

vlan 254

name Native

vlan 255

name Management

vlan 299

name BlackHole




interface vlan 10
description External_Servers
ip add 172.19.8.253 255.255.254.0
standby 10 ip 172.19.9.254                  !- o ip a ser usado que é o ultimo ip da rede (/23)
Standby 10 priority 105                     !- como e mais alto que o default quer dizer que e o activo
Standby 10 preempt                          !- caso for abaixo quando voltar a estar up passa a ser o default (SEMPRE que estaja up vai ser o default)
Exit

Interface vlan 11
description Internal_Servers
ip add 172.19.10.253 255.255.254.0
standby 101 ip 172.19.11.254
Standby 11 priority 105
Standby 11 preempt
Exit

Interface vlan 255
description Management
ip add 172.19.12.253 255.255.255.0
standby 255 ip 172.19.12.254
Standby 255 priority 105
Standby 255 preempt
Exit

int vlan 100
description Engineering
ip add 172.19.4.253 255.255.254.0
standby 100 ip 172.19.5.254
ip helper-address 172.19.10.1
exit

int vlan 101
description Administration
ip add 172.19.6.253 255.255.254.0
standby 101 ip 172.19.7.254
ip helper-address 172.19.10.1
exit

int vlan 200
description Voice
ip add 172.19.0.253 255.255.252.0
standby 200 ip 172.19.3.254                !- este é /22 logo o ultimo ip e .3.254
ip helper-address 172.19.10.1
exit

interface FastEthernet0/1
Description Link to Vienna
No switchport
Ip address 172.19.13.2 255.255.255.0
duplex auto
speed auto
no shutdown
exit



