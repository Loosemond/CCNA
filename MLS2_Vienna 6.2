en
conf t
hostname MLS2_Vienna
username cisco privilege 15 secret class
banner login !Efetue um backup de segurança antes de efetuar alteracoes!
ip domain name MLS2_Vienna.CCNA_lab.com
crypto key generate rsa general-keys modulus 1024 expo
line con 0
login local
exit
line vty 0 4
privilege level 15
login local
transport input ssh
exit


!--MPLS
ip routing
ip cef
!mpls ip


! ----- Services
ip dhcp snooping

ip default-gateway 172.19.12.3
service password-encryption

!---- VLANS
vlan 10

name External_Servers

vlan 11
name Internal_Servers

vlan 200

name Voice

vlan 100

name Engineering

vlan 101

name Administration

vlan 254

name Native

vlan 255

name Management

vlan 299

name BlackHole

vlan 201

name Wireless

vlan 202

name Wireless-Guest


!---Interfaces

!--MLS1_Vienna
conf t
interface range FastEthernet 0/23-24
channel-group 1 mode active
Exit
interface Port-channel 1
switchport trunk encapsulation dot1q
switchport trunk native Vlan 254
switchport mode trunk
switchport trunk allowed vlan 10,11,255,100,101,200,201,202

!--SW1_Vienna
interface range FastEthernet 0/21-22
channel-group 2 mode active
Exit
interface Port-channel 2
switchport trunk encapsulation dot1q
switchport trunk native Vlan 254
switchport mode trunk
switchport trunk allowed vlan 10,11,255,100,101,200,254,201,202
ip dhcp snooping trust
ip dhcp snooping limit rate 10

!--SW2_Vienna
interface range FastEthernet 0/19-20
channel-group 3 mode active
Exit
interface Port-channel 3
switchport trunk encapsulation dot1q
switchport trunk native Vlan 254
switchport mode trunk
switchport trunk allowed vlan 10,11,255,100,101,200,254,201,202
ip dhcp snooping trust
ip dhcp snooping limit rate 10


interface vlan 10
description External_Servers
ip add 172.19.9.252 255.255.254.0
standby 10 ip 172.19.9.254                  !- o ip a ser usado que é o ultimo ip da rede (/23)
Standby 10 priority 100
Standby 10 preempt
ip ospf 1 area 3
Exit

Interface vlan 11
description Internal_Servers
ip add 172.19.11.252 255.255.254.0
standby 101 ip 172.19.11.254
Standby 101 priority 100
Standby 101 preempt
ip ospf 1 area 3
Exit

Interface vlan 255
description Management
ip add 172.19.12.252 255.255.255.0
standby 255 ip 172.19.12.254
Standby 255 priority 100
Standby 255 preempt
ip ospf 1 area 3
ip ospf 1 area 0
ip ospf message-digest-key 1 md5 netacad
ip ospf hello-interval 20
ip ospf dead-interval 80
Exit

int vlan 100
description Engineering
ip add 172.19.5.252 255.255.254.0
standby 100 ip 172.19.5.254
Standby 100 priority 110
ip helper-address 172.19.10.1
ip ospf 1 area 3
exit

int vlan 101
description Administration
ip add 172.19.7.252 255.255.254.0
standby 101 ip 172.19.7.254
Standby 101 priority 110
Standby 101 preempt
ip helper-address 172.19.10.1
ip ospf 1 area 3
exit

int vlan 200
description Voice
ip add 172.19.3.252 255.255.252.0
standby 200 ip 172.19.3.254               !- este é /22 logo o ultimo ip e .3.254
Standby 200 priority 110
Standby 200 preempt
ip helper-address 172.19.10.1
ip ospf 1 area 3
exit


int vlan 201
description WIRELESS
ip add 172.19.14.252 255.255.255.0
standby 201 ip 172.19.14.254                !- este é /22 logo o ultimo ip e .3.254
Standby 201 priority 110
Standby 201 preempt
ip ospf 1 area 3
ip helper-address 172.19.10.1
exit


int vlan 202
description WIRELESS-GUESTS
ip add 172.19.15.252 255.255.255.0
standby 202 ip 172.19.15.254                !- este é /22 logo o ultimo ip e .3.254
Standby 202 priority 110
Standby 202 preempt
ip ospf 1 area 3
ip helper-address 172.19.10.1
exit


interface fa0/1
Description Link to Vienna
No switchport
Ip address 172.19.13.6 255.255.255.252
ip ospf 1 area 3
ip ospf 1 area 0
ip ospf message-digest-key 1 md5 netacad
ip ospf hello-interval 20
ip ospf dead-interval 80
duplex auto
speed auto
no shutdown
exit

!---OSPF
router ospf 1
no passive-interface vlan 255
no passive-interface fa0/1
router-id 1.1.1.5
area 0 authentication message-digest
exit


! ----- Spanning Tree
spanning-tree mode rapid-pvst
spanning-tree vlan 100 root primary
spanning-tree vlan 101 root primary
spanning-tree vlan 200 root primary
spanning-tree vlan 201 root primary
spanning-tree vlan 202 root primary
spanning-tree vlan 10 root secondary
spanning-tree vlan 11 root secondary
spanning-tree vlan 255 root secondary

