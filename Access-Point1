enable
config t
hostname ap1
ip domain name AP1_Vienna.CCNA_lab.com
crypto key generate rsa general-keys modulus 1024 expo
line con 0
login local
exit
line vty 0 4
privilege level 15
login local
transport input ssh
exit

ip default-gateway 172.19.12.254


dot11  ssid WIRELESS
VLAN  201
authentication open
authentication key-Management wpa
mbssid Guest-mode
wpa-psk ascii ciscoclass
exit

interface Dot11Radio0
no shutdown
encryption vlan 201 mode ciphers aes-ccm tkip
ssid WIRELESS
mbssid
Exit

interface Dot11Radio0.201
encapsulation dot1Q 201
bridge-group 201
Exit

interface Dot11Radio1
no shutdown
encryption vlan 201 mode ciphers aes-ccm tkip
ssid WIRELESS
mbssid
Exit

interface Dot11Radio1.201
encapsulation dot1Q 201
bridge-group 201
exit

interface FastEthernet0
no shutdown
Exit

interface FastEthernet0.201
encapsulation dot1Q 201
bridge-group 201
exit

interface FastEthernet0.254
encapsulation dot1Q 254 native
bridge-group 1
Exit

interface FastEthernet0.255
ip address 172.19.12.3 255.255.255.0
encapsulation dot1Q 255
bridge-group 255
Exit

interface BVI1
no ip address
Exit

interface BVI255
ip address 172.19.12.3 255.255.255.0
Exit
